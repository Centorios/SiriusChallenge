{
    "extends": [
        "next/core-web-vitals",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": 2,
        "@typescript-eslint/no-non-null-assertion": 2,
        "@typescript-eslint/no-unused-vars": [
            2,
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ]
    }
}
